#============= qti_init_shell ==============
allow qti_init_shell vendor_radio_data_file:dir search;
allow qti_init_shell { sysfs_leds sysfs }:file { setattr write };
allow qti_init_shell persist_file:dir { getattr search };
allow qti_init_shell proc_boot_reason:file r_file_perms;
allow qti_init_shell proc_cmdline:file r_file_perms;
allow qti_init_shell proc_watermark_scale_factor:file { open write };

allow qti_init_shell {
  ctl_start_prop
  ctl_stop_prop
  vendor_boardid_prop
}:property_service set;

dontaudit qti_init_shell self:capability { dac_override dac_read_search };

